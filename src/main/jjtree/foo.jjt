}
{
	LOOKAHEAD( 4 )
	<FOR> " " dest=lvn() "=" node1=expr() ( ":" node2=expr() )? ( ":" node3=expr() )?
	{
		ForCommand node = new ForCommand();
		node.setDestination(dest);
		node.setExpression1(node1);
		node.setExpression2(node2);
		node.setExpression3(node3);
