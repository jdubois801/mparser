    ( n1=<NAME> | n1=<BREAK> | n1=<CLOSE> | n1=<DO> | n1=<ELSE> | n1=<FOR> | n1=<GOTO> | n1=<HALT> | n1=<IF> | n1=<JOB> | n1=<KILL> | n1=<LOCK> | n1=<MERGE> | n1=<NEW> | n1=<OPEN> | n1=<QUIT> | n1=<READ> | n1=<SET> | n1=<USE> | n1=<VIEW> | n1=<WRITE> | n1=<XECUTE> )
