Expression expritem() : {
Expression result = null;
Expression node1 = null;
Token n1 = null;
}
{
// TODO - are all of these implemented?
// strlit
// numlit
// exfunc
// exvar   $externref or $$labelref
// svn   $X, $DEVICE, and so on...
// function
// unaryop expratom
// ( expr )

	n1=<NUMBER> 
    {
        return new NumericConstant(n1.toString());
    }
    | n1=<STRLIT>
    {
        return new StringConstant(n1.toString());
    }
    | n1=unaryop() node1=expratom()
    {
        return new UnaryExpression(n1.toString(), node1);
    }
    | "(" node1=expr() ")"
    {
        return node1;   
    }
    | node1=function()
    {
        return node1;
    }
}

Token unaryop() : {
Token n1 = null;
}
{
	( n1="\'" | n1="+" | n1="-" )
    {
        return n1;
    }
}

Token binaryop() : {
Token n1 = null;
}
{
	( n1="_" | n1="+" | n1="-" | n1="*" | n1="/"| n1="#" | n1="\\" | n1="**" )
    {
       return n1;
    }
}

Token relation() : {
Token n1 = null;
}
{
	( n1="=" | n1="<" | n1=">" | n1="]" | n1="[" | n1="]]" )
    {
       return n1;
    }
}

Token logicalop() : {
Token n1 = null;
}
{
	( n1="&" | n1="!" )
	{
        return n1;
    }
}
