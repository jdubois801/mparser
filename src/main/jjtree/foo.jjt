}
{
	"@" node1=expratom()
	{
	    result.setExpression(new IndirectExpression(node1));
	    return result;
	}
	| node1=expr() ( node2=postcond() )?
    {
        result.setExpression(node1);
        result.setPostCondition(node2);
        return result;
    }
}

